<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Name="LabelTextBlockStylesRes">
	<Style
		TargetType="{x:Type Label}">
		<Setter
			Property="Foreground"
			Value="White" />
		<Setter
			Property="Padding"
			Value="0" />
	</Style>
	<Style
		TargetType="{x:Type TextBlock}">
		<Setter
			Property="Foreground"
			Value="White" />
		<Setter
			Property="Padding"
			Value="0" />
	</Style>
</ResourceDictionary>